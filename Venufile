pipeline {
  agent any

  tools{
    maven 'M2_HOME'
    }
stages {
stage('Git Checkout') {
 steps {
   echo 'This is for cloning gitrepo'
   git branch: 'main', url: 'https://github.com/venugopal-aadi/insure_me.git'
                       }
}
stage('Create Package') {
 steps {
  sh 'mvn package' 
                        }
}
}
}
